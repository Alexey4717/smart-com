{
  "name": "smart-com",
  "author": "Alexey Moiseenko",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.68",
    "@mui/material": "^5.4.1",
    "@mui/system": "^5.4.1",
    "@reduxjs/toolkit": "^1.7.2",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.3",
    "formik": "^2.2.9",
    "notistack": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.5",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "check-types": "tsc --skipLibCheck --noEmit",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx src/ --cache --fix --quiet",
    "prepare": "rm -rf .husky && cd ../.. && husky install src/rk-rao/.husky && cd src/rk-rao && npx husky add .husky/pre-commit \"cd src/rk-rao && npx lint-staged\""
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.5.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^11.2.3",
    "prettier": "2.4.1"
  },
  "optionalDependencies": {
    "husky": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "bash -c 'npm run check-types'",
      "eslint  --cache --fix"
    ]
  }
}
